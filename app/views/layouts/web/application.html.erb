<!DOCTYPE html>
<html>


    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Lokusapp" %></title>
    <meta name="description" content="<%= content_for?(:description) ? yield(:description) : "Lokusapp" %>">
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <%= render 'layouts/web/navigation' %>
    </header>
    <main role="main">
      <%= render 'layouts/web/navigation_top' %>
      <%= render 'layouts/web/navigation_bottom' %>
       <%= render 'layouts/web/messages' %>
       <div id="page-header-bottom-holder" class="page-header-bottom-holder"></div>
       <%= yield %>

    </main>
  </body>
</html>
